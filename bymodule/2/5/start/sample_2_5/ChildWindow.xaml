<UserControl x:Class="sample_2_5.ChildWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:sample_2_5"
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" mc:Ignorable="d"
        d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel>
        <dxb:MainMenuControl DockPanel.Dock="Top">
            <dxb:BarSubItem Content="File" MergeType="MergeItems">
                <dxb:BarButtonItem Content="MDI Child Item" />
            </dxb:BarSubItem>
        </dxb:MainMenuControl>
        <dxb:ToolBarControl DockPanel.Dock="Top" Caption="Child Bar">
            <dxb:BarButtonItem Content="MDI Child Button" />
        </dxb:ToolBarControl>
    </DockPanel>
</UserControl>
